<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.nsu.ccfit.hwr.recognition"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="true"
        >
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.TouchWriterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.TrainingSetToolActivity"
            android:label="@string/title_activity_training_set_tool" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.EquationEnterActivity"
            android:label="@string/title_activity_equation_enter" >
        </activity>
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.StartActivity"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.SolvedStageActivity"
            android:label="@string/title_activity_solved_stage" >
        </activity>
        <activity
            android:name="ru.nsu.ccfit.hwr.activity.ParsedStageActivity"
            android:label="@string/title_activity_parsed_stage" >
        </activity>
    </application>

</manifest>
